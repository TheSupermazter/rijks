<%- include('partials/head') %>

<body class="favo">
    <%- include('partials/header') %>

    <main>
      <section>
        <h2>Nog te zoeken</h2>
        <section>
            <% combinedDetails.forEach(function(object) { %>
                <% if (!object.objectFound) { %>
                    <% const artwork = object.details.artObject; %>
                    <article>
                      <a class="knop" href="/info/<%= artwork.objectNumber %>">
                        <img class="favoimg" src="<%= artwork.webImage.url %>" alt="Image <%= artwork.title %>">
                        <p><%= artwork.title %></p>
                      </a>
                    </article>
                <% } %>
            <% }); %>
        </section>
    </section>
    
    

      <article id="search-container">
        <input type="text" id="search-input" placeholder="Zoek naar iets...">
        <button onclick="zoek()">Zoek</button>
      </article>
    <article id="resultaten-container">
        
    </article>
      
    <section>
      <h2>Gevonden werken</h2>
      <section>
          <% combinedDetails.forEach(function(object) { %>
              <% if (object.objectFound) { %>
                  <% const artwork = object.details.artObject; %>
                  <article>
                    <a class="knop" href="/info/<%= artwork.objectNumber %>">
                      <img class="favoimg" src="<%= artwork.webImage.url %>" alt="Image <%= artwork.title %>">
                      <p><%= artwork.title %></p>
                    </a>
                  </article>
              <% } %>
          <% }); %>
      </section>
  </section>
    </main>

    <%- include('partials/footer') %>

    <script src="/script/ryan.js"></script>
 
</body>

</html>